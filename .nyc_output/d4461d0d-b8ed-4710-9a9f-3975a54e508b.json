UX,YAAYO,GAA5B;;AAEAb,aAAWQ,OAAX;;AAEAR,aAAWkB,IAAX,CAAgBZ,SAAhB;;AAEAN,eAAaA,WAAWmB,IAAX,CAAgB,EAAhB,CAAb;;AAEA,MAAIF,UAAU,EAAV,KAAiB,CAArB,EAAwB;AACtB,WAAO,IAAP;AACD,GAFD,MAEO;AACL,WAAO,KAAP;AACC;AAEJ,CA3CH;;AA6CEG,QAAQC,GAAR,CAAYtB,cAAc,qBAAd,CAAZ;;QAESA,a,GAAAA,a","file":"index.js","sourceRoot":"/Users/anapaulalopesaraujo/Documents/github/card-number-validator-lib","sourcesContent":["const cardValidator = (cardNumber) => {\n\n    cardNumber = cardNumber.replace(/\\D/g, \"\");\n  \n    if (cardNumber === \"0000000000000000\" ||\n        cardNumber === \"1111111111111111\" ||\n        cardNumber === \"2222222222222222\" ||\n        cardNumber === \"3333333333333333\" ||\n        cardNumber === \"4444444444444444\" ||\n        cardNumber === \"5555555555555555\" ||\n        cardNumber === \"6666666666666666\" ||\n        cardNumber === \"7777777777777777\" ||\n        cardNumber === \"8888888888888888\" ||\n        cardNumber === \"9999999999999999\") {\n      return false\n    }\n  \n    cardNumber = cardNumber.split('').map(digit => parseInt(digit))\n  \n    const lastDigit = cardNumber.pop()\n  \n    cardNumber.reverse()\n  \n    const multiplyingArray = cardNumber.map((item, index) => index % 2 === 0 ? cardNumber[index] * 2 : cardNumber[index])\n  \n    const multiplyNumbersForTwo = multiplyingArray.map((item, index) => multiplyingArray[index] > 9 ? multiplyingArray[index] - 9 : multiplyingArray[index])\n  \n    let sum = multiplyNumbersForTwo.reduce((total, value) => total + value)\n  \n    const isValid = lastDigit + sum;\n  \n    cardNumber.reverse()\n  \n    cardNumber.push(lastDigit)\n  \n    cardNumber = cardNumber.join('')\n  \n    if (isValid % 10 === 0) {\n      return true\n    } else {\n      return false\n      }\n  \n  }\n  \n  console.log(cardValidator(\"0000 0000 0000 0000\"))\n\n  export { cardValidator }"]},"_coverageSchema":"1a1c01bbd47fc00a2c39e90264f33305004495a9","hash":"d4563901fb28a34efe59a980d4b1da866606ab18","contentHash":"9fa3e2a21b929